files:
  "/etc/cron.d/laravel_scheduler":
    mode: "000644"
    owner: root
    group: root
    content: |
      * * * * * root . /opt/elasticbeanstalk/deployment/env && /usr/bin/php  /var/www/html/artisan schedule:run 1>> /var/laralog.log 2>&1

commands:
  remove_old_cron:
    command: "rm -f /etc/cron.d/*.bak"
